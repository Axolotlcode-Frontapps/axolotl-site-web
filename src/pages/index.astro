---
import Layout from '@/layouts/Layout.astro';
import { t, changeLanguage } from 'i18next';
import { Home } from '@/components/home';
import { ContactHome } from '@/components/home/contactHome';
import Offers from '@/components/home/offers.astro';
import Comments from '@/components/home/comments.astro';
import WhoIs from '@/components/home/who-is.astro';

changeLanguage('es');
---

<Layout title={t('home.title-page')}>
  <Home client:load client:only="react" />
  <Offers />
  <Comments />
  <WhoIs />

  <ContactHome client:visible />
</Layout>
